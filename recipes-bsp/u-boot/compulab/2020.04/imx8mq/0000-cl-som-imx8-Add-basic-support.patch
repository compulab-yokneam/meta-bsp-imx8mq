+	imx_iomux_v3_setup_multiple_pads(usbmux_pads, ARRAY_SIZE(usbmux_pads));
+
+	gpio_request(IMX_GPIO_NR(1, 4), "usb_mux");
+	gpio_direction_output(IMX_GPIO_NR(1, 4), 0);
